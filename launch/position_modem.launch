<launch>
	<!-- MODEM -->
    <!-- driver -->
    <node name="modem_driver" pkg="evologics_ros" type="evologics_node.py" output="screen">
    <rosparam command="load" file="$(find evologics_ros)/conf/param_modem.yaml" />
    </node>

	<!--acoustic_link-->
	<node pkg="acoustic_link" name="modem" type="acoustic_link_node" output="screen">
		<rosparam command="load" file="$(find acoustic_link)/conf/topics_modem.yaml"/>
		<remap from="/im/in" to="/modem/im/in"/>
		<remap from="/im/out" to="/modem/im/out"/>
	</node> 

	<!--new position node:
	1-Subscribe modem-acoustic position and publish the transform
	2-Subscribe usbl-gps position and publish the transform
	-->
</launch>
